$('#guessMap').css('zIndex',6);
$('#guessScore').css('zIndex',7);
$('#distance').append('<%= @distance.to_i %> mi');
<% if current_user %>
  $('#scoreValue').append('<%= current_user.scores.last.points %>');
  $('#avg_score').text('<%= current_user.avg_score %>');
  $('#high_score').text('<%= current_user.high_score %>');
  $('#low_score').text('<%= current_user.low_score %>');
<% end %>
